import java.util.Scanner;

public class Main {
  public static void main(String[] args) {
    int val;
    Scanner entrada = new Scanner(System.in);
    System.out.println("Escreva um valor: ");
    val = entrada.nextInt();
    
    if(val>=1&&val<=10){
      for(int i=1;i<11;i++){
        System.out.println(val+"×"+i+" = "+val*i);
      }}
      
      else{
      System.out.println("Número não aceito");
      }}}
